{
  "name": "smil_aarhus",
  "version": "0.1.0",
  "private": true,
  "description": "SMIL AARHUS Website BETA",
  "scripts": {
    "setup": "yarn && yarn setup:env auto && yarn setup:db && yarn setup:packages",
    "setup:env": "node ./scripts/setup_env.cjs",
    "setup:db": "node ./scripts/setup_db.cjs",
    "start": "node ./scripts/start.cjs",
    "clean": "node ./scripts/clean.cjs",
    "setup:packages": "yarn workspaces foreach -vt run setup",
    "lint": "tsc -b && yarn prettier:all --check && yarn eslint .",
    "lint:fix": "yarn eslint --fix . && yarn prettier:all --write",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx,.graphql",
    "prettier:all": "prettier --ignore-path .eslintignore \"**/*.{js,jsx,ts,tsx,graphql,vue}\" --write",
    "dev": "yarn && tsc -b && concurrently --kill-others --names \"TSC,WATCH,RUN,TEST\" --prefix \"({name})\" --prefix-colors \"yellow.bold,yellow.bold,cyan.bold,greenBright.bold\" \"tsc -b --watch --preserveWatchOutput\" \"yarn workspaces foreach -pvi run watch\" \"yarn workspaces foreach -pvi run dev\"",
    "build": "yarn clean && yarn workspaces foreach -vt run build",
    "licenses": "yarn --silent licenses generate-disclaimer > LICENSES.md",
    "--shortcuts to run commands in workspaces--": "",
    "admin": "yarn workspace @app/admin",
    "client": "yarn workspace @app/client",
    "config": "yarn workspace @app/config",
    "db": "yarn workspace @app/db",
    "graphql": "yarn workspace @app/graphql",
    "server": "yarn workspace @app/server",
    "editor": "yarn workspace @app/editor",
    "docker": "yarn --cwd ./docker",
    "docker-compose": "yarn --cwd ./docker compose"
  },
  "author": "Jasmin Bom",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "concurrently": "^7.0.0",
    "dayjs": "^1.10.7",
    "depcheck": "^1.4.3",
    "dotenv": "^14.3.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.4.0",
    "eslint_d": "^11.1.1",
    "graphql": "^15.8.0",
    "ical.js": "^1.5.0",
    "inquirer": "^8.2.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "vue-eslint-parser": "^8.2.0"
  },
  "resolutions": {
    "graphql": "15.x",
    "pg-connection-string": "2.x",
    "@graphql-typed-document-node/core": "3.1.1",
    "vfonts": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.3.tgz",
    "highlight.js": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.3.tgz",
    "util": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.3.tgz",
    "crypto-browserify": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.3.tgz"
  },
  "workspaces": {
    "packages": [
      "@app/*"
    ]
  },
  "type": "module",
  "packageManager": "yarn@3.1.1"
}
